{
    "name": "@olhapi/frontend",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite --port 3000",
        "start": "vite --port 3000",
        "build": "vite build && tsc",
        "serve": "vite preview",
        "storybook:run": "storybook dev -p 6006",
        "storybook:build": "storybook build",
        "build-storybook": "pnpm run storybook:build",
        "test": "vitest run",
        "test:unit": "vitest run --project=unit",
        "test:storybook": "vitest run --project=storybook",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "chromatic": "npx chromatic --exit-once-uploaded",
        "lint": "oxlint src",
        "check:types": "tsc --noEmit",
        "analyze": "vite build --mode analyze && npx serve dist -p 8080",
        "analyze:bundle": "vite build --mode analyze",
        "lingui:extract": "lingui extract",
        "lingui:compile": "lingui compile",
        "lingui:watch": "lingui extract --watch"
    },
