{
  "name": "@olhapi/api",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "type": "module",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tsc -p test/tsconfig.json && c8 node --test \"test/**/*.ts\"",
    "start": "node app.ts",
    "dev": "npm run kill:dev && fastify start -w -l info app.ts",
    "kill:dev": "lsof -ti :3001 | xargs -r kill -9 || true",
    "docker:dev": "npm run dev",
    "docker:start": "npm run start",
    "lint": "oxlint .",
    "check:types": "tsc --noEmit",
    "setup:s3": "node --env-file=.env scripts/setup-s3.ts",
    "verify:s3": "node --env-file=.env scripts/verify-s3.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.962.0",
    "@aws-sdk/s3-request-presigner": "^3.962.0",
    "@fastify/autoload": "^6.3.1",
    "@fastify/cors": "^11.2.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/multipart": "^9.3.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/sensible": "^6.0.4",
    "@olhapi/email-templates": "workspace:*",
    "@sentry/node": "^10.32.1",
    "@upyo/sendgrid": "^0.4.0",
    "autumn-js": "^0.1.63",
    "better-auth": "^1.4.10",
    "better-upload": "^2.0.3",
    "close-with-grace": "^2.4.0",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "fastify": "^5.6.2",
    "fastify-cli": "^7.4.1",
    "fastify-plugin": "^5.1.0",
    "fastify-type-provider-zod": "^6.1.0",
    "ioredis": "^5.9.0",
    "pg": "^8.16.3",
    "pino-pretty": "^13.1.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "c8": "^10.1.3",
    "concurrently": "^9.2.1",
    "drizzle-kit": "^0.31.8",
    "fastify-tsconfig": "^3.0.0",
    "typescript": "~5.9.3"
  }
}
