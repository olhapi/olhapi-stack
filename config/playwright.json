  "browser": {
    "browserName": "chromium",
    "isolated": false,
    "userDataDir": "tmp/playwright",
    "contextOptions": {
      "viewport": { "width": 1280, "height": 720 }
    }
  },
  "capabilities": ["tabs", "pdf", "vision"],
  "outputDir": "tmp/playwright",
  "imageResponses": "allow"
}
