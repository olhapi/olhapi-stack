{
    "mcpServers": {
        "playwright": {
            "type": "stdio",
            "command": "npx",
            "args": ["@playwright/mcp@latest", "--config", "config/playwright.json"],
            "env": {}
        },
        "context7": {
            "type": "http",
            "url": "https://mcp.context7.com/mcp"
        },
        "sonarqube": {
            "command": "npx",
            "args": ["-y", "sonarqube-mcp-server@latest"],
            "env": {
                "SONARQUBE_URL": "https://sonarcloud.io",
                "SONARQUBE_TOKEN": "${SONARQUBE_TOKEN}",
                "SONARQUBE_ORGANIZATION": "olhapi"
            }
        }
    }
}
